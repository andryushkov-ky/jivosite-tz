.toast {
    position: absolute;
    display: none;
    width: 70px;
    height: 30px;
    z-index: 1;
    border-radius: 7px;
    font-size: 15px;
    font-weight: bold;
    justify-content: center;
    align-items: center;
    opacity: 0;
    border: 1px solid #e6db74;
    color: #23233b;
    background-color: #ffffffdb;
}